{% for message in channel.messages %}  
<div class="card msg-{% if message.user == app.user %}mine{% else %}other{% endif %} m-2">
    <div class="card-header">
    {{ message.user }}

    {% if app.user == message.user %}
    <a href="{{ path('message_delete', {id: message.id}) }}" class="float-right text-danger">&nbsp;[X]</a>
    {% endif %}

    <span class="badge badge-primary float-right">
    {{ message.date|ago }}
    </span>
    </div>
    <div class="card-body">
    {{ message.message }}
    </div>
</div>
{% endfor %}